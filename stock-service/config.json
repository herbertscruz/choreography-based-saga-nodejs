{
    "rabbitmq": {
        "url": "amqp://localhost"
    },
    "mongo": {
        "url": "mongodb://root:example@localhost:27016/",
        "dbName": "stock"
    },
    "api": {
        "port": 3004
    },
    "exchange": "STOCK_TOPIC",
    "queues": {
        "order": {
            "name": "STOCK_ORDER_QUEUE",
            "routingKey": ["order.*"],
            "type": "queue"
        },
        "shipment": {
            "name": "STOCK_SHIPMENT_QUEUE",
            "routingKey": ["shipment.registered"],
            "type": "queue"
        },
        "event": {
            "name": "EVENT_QUEUE",
            "routingKey": ["#"],
            "type": "queue"
        },
        "orderTopic": {
            "name": "ORDER_TOPIC",
            "routingKey": ["stock.unreserved"],
            "type": "exchange"
        }
    }
}