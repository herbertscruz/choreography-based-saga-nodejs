{
    "rabbitmq": {
        "url": "amqp://localhost"
    },
    "mongo": {
        "url": "mongodb://root:example@localhost:27017/",
        "dbName": "order"
    },
    "api": {
        "port": 3000
    },
    "exchange": "ORDER_TOPIC",
    "queues": {
        "stock": {
            "name": "ORDER_STOCK_QUEUE",
            "routingKey": ["stock.unreserved"],
            "type": "queue"
        },
        "invoice": {
            "name": "ORDER_INVOICE_QUEUE",
            "routingKey": ["invoice.*"],
            "type": "queue"
        },
        "event": {
            "name": "EVENT_QUEUE",
            "routingKey": ["#"],
            "type": "queue"
        },
        "stockTopic": {
            "name": "STOCK_TOPIC",
            "routingKey": ["order.*"],
            "type": "exchange"
        }
    }
}