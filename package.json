{
  "name": "choreography-based-saga-nodejs",
  "version": "1.0.0",
  "description": "Proof of concept for Choreography-based saga in NodeJS",
  "main": "src/app.ts",
  "dependencies": {
    "amqplib": "^0.8.0",
    "body-parser": "^1.19.1",
    "express": "^4.17.2",
    "lodash": "^4.17.21",
    "mongodb": "^4.3.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "concurrently": "^7.0.0",
    "eslint": "^8.8.0",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "start:all": "concurrently --kill-others \"npm run start:order\" \"npm run start:payment\" \"npm run start:shipment\" \"npm run start:stock\" \"npm run start:event\"",
    "start:order": "node order-service/index.js",
    "start:payment": "node payment-service/index.js",
    "start:shipment": "node shipment-service/index.js",
    "start:stock": "node stock-service/index.js",
    "start:event": "node dist/event-service/app.js",
    "start": "npm run compile && concurrently --kill-others \"node dist/order-service/app.js\" \"node dist/event-service/app.js\"",
    "compile": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/herbertscruz/choreography-based-saga-nodejs.git"
  },
  "author": "Herberts Cruz <herbertscruz@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/herbertscruz/choreography-based-saga-nodejs/issues"
  },
  "homepage": "https://github.com/herbertscruz/choreography-based-saga-nodejs#readme"
}
